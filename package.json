{
    "name": "ssc-parser",
    "version": "1.0.3",
    "description": "A PEGJS-based parser for Stepmania SSC files",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "repository": {
        "type": "git",
        "url": "https://github.com/piulin/pegjs-ssc-parser"
    },
    "scripts": {
        "build": "rimraf dist && yarn tsc && node addLicense.js",
        "test": "jest",
        "format": "prettier --config .prettierrc '**/*.ts' --write",
        "lint": "eslint src/ --ext .ts",
        "lint:fix": "yarn lint --fix"
    },
    "keywords": [
        "SSC",
        "Stepmania",
        "Javascript",
        "parser",
        "PEGJS"
    ],
    "author": "piulin <pepo_gonba@hotmail.com>",
